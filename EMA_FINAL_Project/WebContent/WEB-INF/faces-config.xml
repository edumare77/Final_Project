<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	
	
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>main.view.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>receptionBean</managed-bean-name>
		<managed-bean-class>main.view.ReceptionViewsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>patientBean</managed-bean-name>
		<managed-bean-class>main.view.PatientViewsBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
	     	<property-name>loginBean</property-name>
	     	<value>#{loginBean}</value>
	    </managed-property>
	</managed-bean>
	
		
	<managed-bean>
		<managed-bean-name>medStaffBean</managed-bean-name>
		<managed-bean-class>main.view.MedStaffViewsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
	     	<property-name>loginBean</property-name>
	     	<value>#{loginBean}</value>
	    </managed-property>
	</managed-bean>
	
	
	
	
	<navigation-rule>
        <from-view-id>*</from-view-id>
         <navigation-case>            
            <from-outcome>/reception/home</from-outcome>
            <to-view-id>/receptionHome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/reception/patientsList</from-outcome>
            <to-view-id>/receptionPatientsList.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>            
            <from-outcome>/reception/patientNew</from-outcome>
            <to-view-id>/receptionPatientNew.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/reception/patientDetail</from-outcome>
            <to-view-id>/receptionPatientDetail.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/reception/patientModify</from-outcome>
            <to-view-id>/receptionPatientModify.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/reception/doctorsList</from-outcome>
            <to-view-id>/receptionDoctorsList.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/reception/nursesList</from-outcome>
            <to-view-id>/receptionNursesList.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>            
            <from-outcome>/reception/medStaffNew</from-outcome>
            <to-view-id>/receptionMedStaffNew.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/reception/medStaffDetail</from-outcome>
            <to-view-id>/receptionMedStaffDetail.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/reception/medStaffModify</from-outcome>
            <to-view-id>/receptionMedStaffModify.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>            
            <from-outcome>/patient/home</from-outcome>
            <to-view-id>/patientHome.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>            
            <from-outcome>/patient/pastAppoList</from-outcome>
            <to-view-id>/patientPastAppoList.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/patient/nextAppoList</from-outcome>
            <to-view-id>/patientNextAppoList.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>            
            <from-outcome>/patient/appoNew</from-outcome>
            <to-view-id>/patientAppoNew.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>            
            <from-outcome>/patient/appoDetail</from-outcome>
            <to-view-id>/patientAppoDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>            
            <from-outcome>/med/home</from-outcome>
            <to-view-id>/medStaffHome.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>            
            <from-outcome>/med/regAppoList</from-outcome>
            <to-view-id>/medStaffRegAppoList.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>            
            <from-outcome>/med/nextAppoList</from-outcome>
            <to-view-id>/medStaffNextAppoList.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>            
            <from-outcome>/med/appoModify</from-outcome>
            <to-view-id>/medStaffAppoModify.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>            
            <from-outcome>/med/appoDetail</from-outcome>
            <to-view-id>/medStaffAppoDetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    <navigation-rule>
        <description>Login Page</description>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>reception</from-outcome>
            <to-view-id>/receptionHome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
         <from-outcome>med_staff</from-outcome>
            <to-view-id>/medStaffHome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>patient</from-outcome>
            <to-view-id>/patientHome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>invalid</from-outcome>
            <to-view-id>/fail_login.xhtml</to-view-id>
        </navigation-case>
     </navigation-rule>
    
    <navigation-rule>
        <description>ReLoging Page</description>
        <from-view-id>/fail_login.xhtml</from-view-id>
        <navigation-case>
       <from-outcome>med_staff</from-outcome>
            <to-view-id>/medStaffHome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>patient</from-outcome>
            <to-view-id>/patientHome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>reception</from-outcome>
            <to-view-id>/receptionHome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>invalid</from-outcome>
            <to-view-id>/fail_login.xhtml</to-view-id>
        </navigation-case>       
    </navigation-rule>
    
    
    <application>
    	<resource-bundle>
    		<base-name>main.application.messages</base-name>
    		<var>msg</var>
    	</resource-bundle>
    </application>
</faces-config>